    "import pandas as pd\n",
    "from google_play_scraper import app\n",
    "from google_play_scraper import Sort,reviews_all\n",
    "reviews = reviews_all(\"com.cjoshppingphone\",\n",
    "                      sleep_milliseconds=0,\n",
    "                      lang = \"ko\",\n",
    "                      country = \"KR\",\n",
    "                      sort = Sort.NEWEST,\n",
